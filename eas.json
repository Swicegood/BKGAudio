{
  "cli": {
    "version": ">= 3.12.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "production-debug": {
      "distribution": "internal",
      "channel": "production-debug",
      "env": {
        "APP_VARIANT": "debug"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "APP_VARIANT": "debug"
      }
    }
  },
  "submit": {
    "production": {}
  }
}